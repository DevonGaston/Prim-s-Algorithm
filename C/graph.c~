void readGraph(char* file){
  FILE * fp;
  char node_1[50], node_2[50], weight[50];
  int weight_num,  r;
  fp = fopen(file, "r");
  if(fp == NULL){
    printf("ERROR: FILE NOT FOUND!\n");
    exit(1);
  }
  while((r = fscanf(fp, "%s, %s, %s\n", node_1, node_2, weight)) != EOF){
    weight_num = atoi(weight);
    printf("%s, %s, %d\n", node_1, node_2, weight_num);
  }
  fclose(fp);
  
}
